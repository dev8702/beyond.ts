{
  "name": "react-isomorphic-serverless-typescript",
  "version": "1.0.0",
  "description": "ReactJS doing isomorphic rendering without server by using serverless framework(AWS Lambda)",
  "main": "index.js",
  "scripts": {
    "pack": "rm -f dist.zip && rm -f dist/bundleBrowser.js && cp scripts/package.json dist/package.json && (cp .npmrc dist/.npmrc || true) && cd dist && npm install --cache=./.npm --production && zip -r ../dist.zip . ** && cd ..",
    "clean": "rm -rf dist && rm -rf temp && rm -f dist.zip",
    "build": "npm run clean && webpack --content-base app/ --config webpack.prod.config.js && npm run build-browser",
    "build-browser": "webpack --content-base app/ --config webpack.browser.config.js",
    "deploy-to-stage": "npm run build && ts-node scripts/deployToStage.ts && cp temp/version dist && cp scripts/handler.js dist && npm run pack && serverless deploy --stage stage",
    "deploy-to-production": "ts-node scripts/deploy-to-prod.ts",
    "dev": "webpack-dev-server --content-base app/ --hot --progress --colors --host 0.0.0.0",
    "lint": "tslint 'app/**/*.ts' -e 'app/**/*.d.ts' && sass-lint -c .sass-lint.yml --verbose",
    "unit-test": "rm -r output | jest --coverage",
    "ssr-test": "npm run build && SSR_TEST=true node dist/bundle.js",
    "test": "npm run lint && npm run unit-test && npm run ssr-test"
  },
  "engines": {
    "node": "6.5.0"
  },
  "author": "Tylor Shin",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-sdk": "^0.0.38",
    "@types/enzyme": "^2.5.37",
    "@types/jest": "^15.1.32",
    "@types/node": "^6.0.62",
    "@types/redux-mock-store": "^0.0.5",
    "autoprefixer": "^6.5.1",
    "aws-sdk": "^2.6.10",
    "css-loader": "^0.25.0",
    "enzyme": "^2.5.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "isomorphic-style-loader": "^1.0.0",
    "jasmine-reporters": "^2.2.0",
    "jest": "^16.0.2",
    "json-loader": "^0.5.4",
    "node-sass": "^3.10.1",
    "postcss-loader": "^1.0.0",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.3.2",
    "redux-mock-store": "^1.2.1",
    "s3": "^4.4.0",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.0.2",
    "serverless": "^1.5.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "svg-inline-loader": "^0.7.1",
    "ts-jest": "^0.1.10",
    "ts-loader": "^0.9.3",
    "ts-node": "^1.6.0",
    "tslint": "^4.0.2",
    "tslint-microsoft-contrib": "^2.0.13",
    "typescript": "^2.1.4",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "@types/axios": "^0.9.32",
    "@types/immutable": "^3.8.4",
    "@types/react": "^0.14.41",
    "@types/react-addons-css-transition-group": "^0.14.17",
    "@types/react-addons-shallow-compare": "^0.14.17",
    "@types/react-dom": "^0.14.18",
    "@types/react-redux": "^4.4.32",
    "@types/react-router": "^2.0.38",
    "@types/react-router-redux": "^4.0.34",
    "@types/redux": "^3.6.31",
    "@types/redux-logger": "^2.6.32",
    "@types/redux-thunk": "^2.1.31",
    "axios": "^0.15.2",
    "babel-polyfill": "^6.20.0",
    "immutable": "^3.8.1",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/scripts/jestPreprocessor.js",
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/jestReporter.js",
    "verbose": true,
    "rootDir": "",
    "globals": {
      "__TS_CONFIG__": "tsconfig-test.json"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathDirs": [
      "<rootDir>/app"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageDirectory": "output/coverage",
    "testRegex": "__tests__/.*_spec.tsx$",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/scripts",
      "<rootDir>/app/common",
      "<rootDir>/",
      "/__mocks__/",
      "/.*\\.scss$",
      "<rootDir>/app/__tests__"
    ],
    "setupFiles": [
      "<rootDir>/app/__tests__/preload.tsx"
    ],
    "automock": true
  }
}
